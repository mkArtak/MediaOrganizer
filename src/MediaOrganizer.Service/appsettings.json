{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "FilesOrganizerOptions": {
    "VideoSubfolderName": "Movies",
    "PhotosSubfolderName": "Photos",
    "SourceRoot": "/volume1/FamilyMedia/Unsorted",
    "DestinationRoot": "/volume1/FamilyMedia/Sorted",
    "DestinationPattern": "{Year}/{MonthName}/{Year}-{Month}-{Day}",
    "RemoveSource": true,
    "SkipExistingFiles": true
  }
}
